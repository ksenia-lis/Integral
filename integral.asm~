section .data
    a dd 14
section .rodata

section .text
global f1, f2
;;;;;lnx
f1:
    push ebp
    mov ebp, esp
    finit
    fld1
    fld dword[esp + 20]
    FYL2X
    fstp dword[esp + 12]
    fld dword[esp + 12]
    FLDL2E
    FDIV
    fstp qword[esp + 12]
    leave 
    xor eax, eax
    ret

;−2x + 14
f2:
    PUSH EBP
    MOV EBP, ESP
    finit
    mov dword[esp + 12], -2
    fild dword[esp + 12] ;-2
    fld dword[esp + 20] ;x
    fmul ;x*(-2)
    mov dword[esp + 12], 14 
    fild dword[esp + 12] ;14
    fadd ;+14
    fstp qword[esp + 12]; ans
    leave 
    RET
;;;;; 1/(2 − x)+6
f3:
    PUSH EBP
    MOV EBP, ESP
    finit
    mov dword[esp + 12], 2
    fld dword[esp + 20];x
    fild dword[esp + 12];2
    fsub ;x - 2
    fstp dword[esp + 20] ; x= x-2
    fld1
    fld dword[esp + 20]
    fdiv
    mov dword[esp + 12], 6
    fild dword[esp + 12] ; 6
    fadd
    fstp qword[esp + 12]
    MOV ESP, EBP
    POP EBP
    RET
;;;;;;;;;;;;;;;;;;;;;;;;;;

